	<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="b81a2984-35da-4c5c-b86c-9cfe45d16c4a" >
		<http:listener-connection host="0.0.0.0" port="8081" />
	</http:listener-config>
	<api-gateway:autodiscovery apiId="18840050" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="38384b19-45d8-467e-971c-b4ecdb6f7dfc" flowRef="helloFlow" />
	<flow name="helloFlow" doc:id="64bc6bc8-ac17-4240-829a-2bf9e734a276" >
		<http:listener doc:name="Listener" doc:id="14210516-2857-49af-94ad-a6a1f5eb8a86" config-ref="HTTP_Listener_config" path="/hello"/>
		<ee:transform doc:name="Transform Message" doc:id="2f017b12-2bb3-4a9a-b67a-99855b5f510d" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
"Hello"]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</flow>
</mule>
